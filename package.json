{
  "name": "@millenniumearl/recaptcha-harvester",
  "version": "1.0.1",
  "description": "Pluggable Captcha harvester with Electron",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc {src,test}/**/*.ts --write",
    "build": "tsc --project ./ && copyfiles -u 1 src/widget/**/* dist && copyfiles security/* dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "example": "npm run build && node --trace-warnings ./dist/example.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MillenniumEarl/recaptcha-harvester.git"
  },
  "keywords": [
    "reCAPTCHA",
    "captcha",
    "harvester"
  ],
  "author": "MillenniumEarl",
  "contributors": [
    "Kelvin Fichter"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MillenniumEarl/recaptcha-harvester/issues"
  },
  "homepage": "https://github.com/MillenniumEarl/recaptcha-harvester#readme",
  "engines": {
    "node": ">=14.10"
  },
  "dependencies": {
    "express": "^4.17.1",
    "electron": "^14.0.0",
    "node-ipc": "<10.0.0",
    "uuid": "^8.3.2",
    "ws": "^8.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node-ipc": "^9.1.5",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.31.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "directories": {
    "test": "test"
  }
}
